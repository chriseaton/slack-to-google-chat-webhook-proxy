{
    "plugins": [
        "plugins/markdown",
        "plugins/underscore",
        "jsdoc-plugin-intersection"
    ],
    "source": {
        "include": ["./"],
        "exclude": ["./node_modules", "./old", "./assets"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "destination": "./docs/",
        "recurse": true
    },
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	},
	"metadata": {
		"title": "Flight Control: Runway",
		"logo": "logo.png"
	}
}